{
  "name": "harness-browser3d",
  "version": "dev",
  "scripts": {
    "ng": "ng",
    "start": "ng serve --open",
    "watch": "ng build --watch --configuration development",
    "build:library:dev": "npm run generate:api && ng build harness-browser3d-library --configuration development",
    "build:library:prod": "npm run generate:api && ng build harness-browser3d-library --configuration production",
    "watch:library": "npm run generate:api && ng build harness-browser3d-library --configuration development --watch",
    "generate:api": "npx openapi-typescript projects/harness-browser3d-library/assets/geometry-api.yaml --output projects/harness-browser3d-library/src/generated/geometry.ts"
  },
  "dependencies": {
    "@angular/animations": "20.3.13",
    "@angular/cdk": "20.2.14",
    "@angular/common": "20.3.13",
    "@angular/compiler": "20.3.13",
    "@angular/core": "20.3.13",
    "@angular/forms": "20.3.13",
    "@angular/material": "20.2.14",
    "@angular/platform-browser": "20.3.13",
    "@angular/platform-browser-dynamic": "20.3.13",
    "@angular/router": "20.3.13",
    "@tailwindcss/postcss": "4.1.17",
    "postcss": "8.5.6",
    "rxjs": "7.8.2",
    "stats.js": "0.17.0",
    "tailwindcss": "4.1.17",
    "three": "0.177.0",
    "tslib": "2.8.1",
    "zone.js": "0.15.1"
  },
  "devDependencies": {
    "@angular/build": "20.3.11",
    "@angular/cli": "20.3.11",
    "@angular/compiler-cli": "20.3.13",
    "@types/stats.js": "0.17.4",
    "@types/three": "0.177.0",
    "ng-packagr": "20.3.2",
    "openapi-typescript": "7.10.1",
    "prettier": "3.6.2",
    "typescript": "5.9.2"
  }
}
